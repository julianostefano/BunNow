{
  "name": "bunsnc",
  "version": "1.0.0",
  "description": "CLI e biblioteca para integração ServiceNow, baseada em Bun/Elysia. Suporte a CRUD, batch, attachments e mock.",
  "main": "dist/bunsnc",
  "types": "src/client/index.ts",
  "exports": {
    ".": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts",
      "require": "./dist/client/index.js"
    },
    "./client": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts",
      "require": "./dist/client/index.js"
    },
    "./types": {
      "types": "./src/client/types.ts",
      "import": "./src/client/types.ts"
    }
  },
  "bin": {
    "bunsnc": "dist/bunsnc"
  },
  "files": [
    "dist/bunsnc",
    "src/client/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ServiceNow/PySNC.git"
  },
  "keywords": [
    "servicenow",
    "cli",
    "elysia",
    "bun",
    "integration",
    "api",
    "sdk",
    "client",
    "eden-treaty",
    "typescript",
    "type-safe",
    "rest-api"
  ],
  "author": "ServiceNow Dev Team <dev@example.com>",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "web": "bun src/web/app.ts",
    "web:dev": "bun --watch src/web/app.ts",
    "web:simple": "bun src/web/simple-server.ts",
    "build": "bun build",
    "compile": "bun compile src/cli/index.ts --out dist/bunsnc",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:performance": "bun test src/tests/performance/",
    "test:client": "bun test src/client/tests/",
    "benchmark": "bun run src/benchmarks/index.ts",
    "demo": "bun examples/client-demo.ts",
    "demo:client": "bun examples/client-demo.ts",
    "lint": "bunx @biomejs/biome check src/",
    "lint:fix": "bunx @biomejs/biome check --apply src/",
    "typecheck": "bun --bun tsc --noEmit",
    "precommit": "bun run lint && bun run typecheck && bun run test",
    "ci": "bun run lint && bun run typecheck && bun run test:coverage && bun run build"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/eden": "^1.3.3",
    "@elysiajs/html": "^0.8.0",
    "@elysiajs/jwt": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@elysiajs/swagger": "^0.8.2",
    "@gtramontina.com/elysia-htmx": "2.0.0",
    "@gtramontina.com/elysia-tailwind": "2.0.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "apache-arrow": "^14.0.2",
    "axios": "^1.6.5",
    "elysia": "^0.8.17",
    "elysia-background": "^1.0.1",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "node-webhdfs": "^1.0.2",
    "parquetjs": "^0.11.2",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "bun-types": "^1.2.0",
    "@biomejs/biome": "^1.4.0",
    "@types/node": "^20.0.0"
  }
}
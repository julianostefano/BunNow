{
  "name": "bunsnc",
  "version": "1.0.0",
  "description": "CLI e biblioteca para integração ServiceNow, baseada em Bun/Elysia. Suporte a CRUD, batch, attachments e mock.",
  "main": "dist/bunsnc",
  "types": "src/client/index.ts",
  "exports": {
    ".": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts",
      "require": "./dist/client/index.js"
    },
    "./client": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts",
      "require": "./dist/client/index.js"
    },
    "./types": {
      "types": "./src/client/types.ts",
      "import": "./src/client/types.ts"
    }
  },
  "bin": {
    "bunsnc": "dist/bunsnc"
  },
  "files": [
    "dist/bunsnc",
    "src/client/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ServiceNow/PySNC.git"
  },
  "keywords": [
    "servicenow",
    "cli",
    "elysia",
    "bun",
    "integration",
    "api",
    "sdk",
    "client",
    "eden-treaty",
    "typescript",
    "type-safe",
    "rest-api"
  ],
  "author": "Juliano Stefano <jsdealencar@ayesa.com> [2025]",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bun src/web/app.ts",
    "web": "bun src/web/app.ts",
    "web:dev": "bun --watch src/web/app.ts",
    "web:simple": "bun src/web/simple-server.ts",
    "build": "bun build",
    "compile": "bun compile src/cli/index.ts --out dist/bunsnc",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:performance": "bun test src/tests/performance/",
    "test:client": "bun test src/client/tests/",
    "benchmark": "bun run src/benchmarks/index.ts",
    "demo": "bun examples/client-demo.ts",
    "demo:client": "bun examples/client-demo.ts",
    "stream:consumer": "bun src/scripts/stream-consumer.ts",
    "hadoop:sync": "bun src/scripts/hadoop-sync.ts",
    "db:migrate": "bun src/scripts/db-migrate.ts",
    "waiting:summary": "bun src/cli/waiting-tickets.ts",
    "waiting:details": "bun src/cli/waiting-tickets.ts --details",
    "test:endpoints": "bun src/tests/cli-endpoint-tests.ts",
    "test:quick": "bun src/tests/cli-endpoint-tests.ts quick-test",
    "test:mapping": "bun src/tests/cli-endpoint-tests.ts analyze-all --generate-interfaces",
    "analyze:tickets": "bun src/tests/ticket-data-analysis.ts",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "bun --bun tsc --noEmit",
    "precommit": "bun run format:check && bun run typecheck && bun run test",
    "ci": "bun run format:check && bun run typecheck && bun run test:coverage && bun run build"
  },
  "dependencies": {
    "@elysiajs/cors": "latest",
    "@elysiajs/eden": "latest",
    "@elysiajs/html": "latest",
    "@elysiajs/jwt": "latest",
    "@elysiajs/static": "latest",
    "@elysiajs/swagger": "latest",
    "@elysiajs/websocket": "latest",
    "@gtramontina.com/elysia-htmx": "latest",
    "@opensearch-project/opensearch": "latest",
    "@tailwindcss/postcss": "latest",
    "apache-arrow": "^21.0.0",
    "axios": "latest",
    "elysia": "latest",
    "elysia-autoload": "^1.7.0",
    "elysia-background": "latest",
    "elysia-decorators": "^1.0.2",
    "elysiajs-helmet": "latest",
    "form-data": "^4.0.4",
    "htmx": "^0.0.2",
    "ioredis": "^5.7.0",
    "lucide": "^0.542.0",
    "mime-types": "^3.0.1",
    "mongodb": "^6.19.0",
    "node-webhdfs": "^1.0.2",
    "parquetjs": "^0.11.2",
    "redis-cli": "^2.1.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "@types/node": "^24.3.0",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.2.21",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  }
}
